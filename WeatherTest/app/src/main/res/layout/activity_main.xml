<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bback" />

    <TextView
        android:id="@+id/tv_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="TextView"
        android:textSize="12sp"
        app:fontFamily="@font/binggrae"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="389dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="3dp"
        android:gravity="bottom"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <LinearLayout
            android:id="@+id/layout_meteo"
            android:layout_width="185dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="@drawable/metro"
            android:orientation="vertical"
            android:scrollbarAlwaysDrawHorizontalTrack="false"
            android:scrollbarAlwaysDrawVerticalTrack="false">

            <TextView
                android:id="@+id/tv_meteo_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:text="TextView"
                android:textColor="#888484"
                app:fontFamily="@font/binggrae"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/iv_meteo_weather"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_gravity="center"
                app:layout_constraintEnd_toEndOf="@+id/tv_meteo_time"
                app:layout_constraintStart_toStartOf="@+id/tv_meteo_time"
                app:srcCompat="@drawable/ic_launcher_foreground"
                tools:ignore="VectorDrawableCompat"
                tools:layout_editor_absoluteY="34dp" />

            <TextView
                android:id="@+id/tv_meteo_temp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:text="TextView"
                android:textColor="#888484"
                android:textSize="18sp"
                app:fontFamily="@font/binggrae"
                app:layout_constraintEnd_toEndOf="@+id/iv_meteo_weather"
                app:layout_constraintStart_toStartOf="@+id/iv_meteo_weather"
                app:layout_constraintTop_toBottomOf="@+id/iv_meteo_weather" />

            <TextView
                android:id="@+id/tv_meteo_weather"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:text="TextView"
                android:textColor="#888484"
                android:textSize="10sp"
                app:fontFamily="@font/binggrae" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="110dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="40dp"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/naverlogo" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="110dp"
                android:layout_height="34dp"
                android:layout_marginLeft="85dp"
                app:srcCompat="@drawable/daumlogo" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/portal"
            android:layout_width="match_parent"
            android:layout_height="180dp">

            <LinearLayout
                android:id="@+id/layout_naver"
                android:layout_width="185dp"
                android:layout_height="match_parent"
                android:layout_margin="5sp"
                android:background="@drawable/metro"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_naver_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:text="TextView"
                    android:textColor="#888484"
                    android:textSize="14sp"
                    app:fontFamily="@font/binggrae"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/iv_naver_weather"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    app:layout_constraintEnd_toEndOf="@+id/tv_naver_time"
                    app:layout_constraintStart_toStartOf="@+id/tv_naver_time"
                    app:layout_constraintTop_toBottomOf="@+id/tv_naver_time"
                    app:srcCompat="@drawable/ic_launcher_foreground"
                    tools:ignore="VectorDrawableCompat" />

                <TextView
                    android:id="@+id/tv_naver_temp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:text="TextView"
                    android:textColor="#888484"
                    android:textSize="18sp"
                    app:fontFamily="@font/binggrae"
                    app:layout_constraintEnd_toEndOf="@+id/iv_naver_weather"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/iv_naver_weather"
                    app:layout_constraintTop_toBottomOf="@+id/iv_naver_weather" />

                <TextView
                    android:id="@+id/tv_naver_weather"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:text="TextView"
                    android:textColor="#888484"
                    android:textSize="10sp"
                    app:fontFamily="@font/binggrae" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_daum"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5sp"
                android:background="@drawable/metro"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_daum_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:text="TextView"
                    android:textColor="#888484"
                    android:textSize="14sp"
                    app:fontFamily="@font/binggrae"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/iv_daum_weather"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    app:layout_constraintEnd_toEndOf="@+id/tv_daum_time"
                    app:layout_constraintStart_toStartOf="@+id/tv_daum_time"
                    app:layout_constraintTop_toBottomOf="@+id/tv_daum_time"
                    app:srcCompat="@drawable/ic_launcher_foreground"
                    tools:ignore="VectorDrawableCompat" />

                <TextView
                    android:id="@+id/tv_daum_temp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:text="TextView"
                    android:textColor="#888484"
                    android:textSize="18sp"
                    app:fontFamily="@font/binggrae"
                    app:layout_constraintEnd_toEndOf="@+id/iv_daum_weather"
                    app:layout_constraintStart_toStartOf="@+id/iv_daum_weather"
                    app:layout_constraintTop_toBottomOf="@+id/iv_daum_weather" />

                <TextView
                    android:id="@+id/tv_daum_weather"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:text="TextView"
                    android:textColor="#888484"
                    android:textSize="10sp"
                    app:fontFamily="@font/binggrae" />

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="32dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="90sp"
            android:layout_height="102dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:paddingLeft="12dp"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/blurr" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="250dp"
            android:layout_height="102dp"
            android:layout_marginTop="30sp"
            android:foregroundGravity="center"
            android:layerType="none"
            android:paddingRight="30dp"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/rwin4" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:layout_marginTop="32dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:srcCompat="@drawable/rrmul" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/meteologo2" />

</androidx.constraintlayout.widget.ConstraintLayout>